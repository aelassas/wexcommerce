{
  "name": "backend",
  "version": "2.8.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo -p 8005",
    "ts:docker-build": "tsc --build --verbose",
    "dev:docker": "npm run ts:docker-build && next dev -p 8005",
    "build": "rimraf .next && tsc --build --verbose && cross-env NODE_OPTIONS=--max-old-space-size=1024 next build",
    "start": "next start -p 8005",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "ncu": "ncu -u",
    "stylelint": "stylelint \"**/*.css\"",
    "stylelint:fix": "stylelint \"**/*.css\" --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-date-pickers": "^8.0.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/draft-js": "^0.11.18",
    "@types/draftjs-to-html": "^0.8.4",
    "@types/html-to-draftjs": "^1.5.0",
    "@types/react-draft-wysiwyg": "^1.13.8",
    "@types/validator": "^13.15.0",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "html-to-draftjs": "^1.5.0",
    "localized-strings": "^2.0.3",
    "next": "^15.3.1",
    "next-nprogress-bar": "^2.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-toastify": "^11.0.5",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@next/eslint-plugin-next": "^15.3.1",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.1",
    "eslint": "^9.25.1",
    "eslint-config-next": "^15.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
