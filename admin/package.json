{
  "name": "wexcommerce-admin",
  "description": "Admin panel for managing wexCommerce",
  "version": "8.9.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo -p 8005",
    "ts:docker-build": "tsc --build --verbose",
    "dev:docker": "npm run ts:docker-build && next dev -p 8005",
    "build": "rimraf .next && tsc --build --verbose && cross-env NODE_OPTIONS=--max-old-space-size=1024 next build",
    "start": "next start -p 8005",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "fix": "eslint --fix",
    "ncu": "ncu -u",
    "stylelint": "stylelint \"**/*.css\"",
    "stylelint:fix": "stylelint \"**/*.css\" --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^8.15.0",
    "@sindresorhus/slugify": "^3.0.0",
    "date-fns": "^4.1.0",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "html-to-draftjs": "^1.5.0",
    "localized-strings": "^2.0.3",
    "next": "^16.0.0",
    "next-nprogress-bar": "^2.4.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-toastify": "^11.0.5",
    "validator": "^13.15.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@next/eslint-plugin-next": "^16.0.0",
    "@types/draft-js": "^0.11.18",
    "@types/draftjs-to-html": "^0.8.4",
    "@types/html-to-draftjs": "^1.5.0",
    "@types/node": "^24",
    "@types/react-draft-wysiwyg": "^1.13.8",
    "@types/validator": "^13.15.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "npm-check-updates": "^19.1.1",
    "rimraf": "^6.0.1",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "^5.9.3"
  }
}
