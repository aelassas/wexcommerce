{
  "name": "frontend",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "install:reactjs-social-login": "cd ../packages/reactjs-social-login && npm i && cd ../../frontend",
    "install:dependencies": "npm run install:reactjs-social-login",
    "ts:build": "npm run install:dependencies && tsc --build --verbose",
    "dev": "npm run ts:build && next dev --turbo -p 8006",
    "build": "rimraf .next && npm run ts:build && cross-env NODE_OPTIONS=--max-old-space-size=1024 next build",
    "start": "next start -p 8006",
    "lint": "next lint",
    "ncu": "ncu -u -x eslint",
    "stylelint": "stylelint \"**/*.css\"",
    "stylelint:fix": "stylelint \"**/*.css\" --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@mui/x-date-pickers": "^7.27.1",
    "@paypal/react-paypal-js": "^8.8.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.7.0",
    "@types/react-slick": "^0.23.13",
    "@types/validator": "^13.12.2",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "github-fork-ribbon-css": "^0.2.3",
    "localized-strings": "^2.0.3",
    "next": "15.1.7",
    "next-nprogress-bar": "^2.4.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-ga4": "^2.1.0",
    "react-slick": "^0.30.3",
    "react-toastify": "^11.0.5",
    "slick-carousel": "^1.8.1",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "babel-plugin-react-compiler": "^19.0.0-beta-e1e972c-20250221",
    "eslint": "^8",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-react-compiler": "^19.0.0-beta-e1e972c-20250221",
    "npm-check-updates": "^17.1.15",
    "rimraf": "^6.0.1",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0"
  }
}
